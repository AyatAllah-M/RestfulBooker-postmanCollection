						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"pm.test(\"Response time is less than 200ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);",
							"});",
							"",
							"// Extract the first bookingid and save it as environment variable 'id'",
							"pm.test(\"Extract first bookingid and save as environment variable 'id'\", function () {",
							"    var jsonData = pm.response.json();",
							"    if (jsonData.length > 0 && jsonData[0].bookingid !== undefined) {",
							"        pm.environment.set(\"id\", jsonData[0].bookingid.toString());",
							"        pm.expect(pm.environment.get('id')).to.eql(jsonData[0].bookingid.toString());",
							"    } else {",
							"        pm.environment.unset('id');",
							"        pm.expect.fail(\"No bookingid found in response\");",
							"    }",
							"});",
